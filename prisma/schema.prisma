generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../public/data.db"
}

model issue {
  id          Int    @id @default(autoincrement())
  year        Int
  name        String
  description String
  years       year   @relation(fields: [year], references: [year], onDelete: NoAction, onUpdate: NoAction)

  @@index([year], map: "idx_issues_year")
}

model year {
  year   Int     @id @default(autoincrement())
  isVote Boolean
  issues issue[]
}
